\DefineFamilyMember[%
  TUHH-MTEC%
]{title}
\renewcommand*{\inittitle}{%
\ifmtecreprt@grayscaletitle
	\definecolor{keycolor1}{gray}{.5}
		\ifx\@titlehead\@empty
			\titlehead{%
				\IfStrEq*{\languagename}{english}%
					{\includegraphics[width=\dimexpr\paperwidth-90mm\relax]{logo/logo_tuhh_gray_en}}%
					{\includegraphics[width=\dimexpr\paperwidth-90mm\relax]{logo/logo_tuhh_gray_de}}%
			}%
		\fi
		\ifx\@mainlogo\@empty
			\mainlogo{%
				\includegraphics[height=12mm]{logo/logo_mtec_gray}
			}%
		\fi
\else
	\definecolor{keycolor1}{RGB}{88, 190, 184} % cyan
	\definecolor{keycolor1}{RGB}{31, 73,125} % mtec-logo-blue
	\ifx\@titlehead\@empty
		\titlehead{%
			\IfStrEq*{\languagename}{english}%
				{\includegraphics[width=\dimexpr\paperwidth-90mm\relax]{logo/logo_tuhh_en}}%
				{\includegraphics[width=\dimexpr\paperwidth-90mm\relax]{logo/logo_tuhh_de}}%
		}%
	\fi
	\ifx\@mainlogo\@empty
		\mainlogo{%
			\includegraphics[height=12mm]{logo/logo_mtec}
		}%
	\fi
\fi
	\ifx\@companylogo\@empty
		\ifx\@companylogofile\@empty%
		\else%
			\companylogo{%
				\includegraphics[height=12mm]{\@companylogofile}
			}%
		\fi
	\fi
  \ifx\@university\@empty
    \university{%
      \tuhhname{}
    }%
  \fi
  \ifx\@faculty\@empty
    \faculty{%
      \mtecname
    }%
  \fi
  \ifx\@place\@empty
    \place{%
      \buildingname{} E | 21073 Hamburg
    }%
  \fi
	\ifx\@website\@empty
    \website{%
      www.tuhh.de/mtec
    }%
  \fi
  \ifx\@titlepagefont\@empty
    \renewcommand*{\@titlepagefont}{\rmfamily}%
  \fi
	
	\IfStrEq{\@subject}{project}{\subjectstr{\projectpapername{}}}{%
		\IfStrEq{\@subject}{seminar}{\subjectstr{\seminarpapername{}}}{%
			\IfStrEq{\@subject}{studentresearch}{\subjectstr{\studentresearchname{}}}{%
				\IfStrEq{\@subject}{diploma}{\subjectstr{\diplomathesisname{}}}{%
					\IfStrEq{\@subject}{degree}{\subjectstr{\degreethesisname{}}}{%
						\IfStrEq{\@subject}{master}{\subjectstr{\masterthesisname{}}}{%
							\IfStrEq{\@subject}{bachelor}{\subjectstr{\bachelorthesisname{}}}{%
								\subjectstr{\projectpapername{}} %
							}%
						}%
					}%
				}%
			}%
		}%
	}%
}
